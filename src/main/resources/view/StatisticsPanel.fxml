<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane styleClass="stack-pane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
      <VBox>
         <children>
            <Label styleClass="label-bright" text="What you might have forgotten to watch:">
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </Label>
            <StackPane VBox.vgrow="ALWAYS">
               <children>
                  <ListView fx:id="forgottenView" />
               </children>
            </StackPane>
         </children>
      </VBox>
    </children>
</StackPane>
